<% provide(:title, "Log in") %>
<div class="row">
<div class="col-sm-4">
</div>
<div class="col-sm-4">
<div class="jumbotron">
<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Log in"%>
<% end %>
</div>
</div>